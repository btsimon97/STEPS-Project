<% provide(:title, 'My Passes') %>
<h1>My Passes</h1>
<div class="center-block">
  <table id="passes" class="table table-hover table-striped table-bordered">
    <thead>
      <tr>
        <th>Student Name</th>
        <th>Date Created</th>
        <th>Time Out</th>
        <th>Destination</th>
        <th>Student Returning?</th>
        <th>Expected Return Time</th>
      </tr>
    </thead>
    <tbody>
      <%= render @passes %>
    </tbody>
  </table>
</div>
